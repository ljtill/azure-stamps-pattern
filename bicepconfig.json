{
    "analyzers": {
        "core": {
            "enabled": true,
            "verbose": false,
            "rules": {
                "no-unused-vars": {
                    "level": "off"
                },
                "use-recent-api-versions": {
                    "level": "info"
                }
            }
        }
    },
    "cloud": {
        "currentProfile": "AzureCloud"
    },
    "experimentalFeaturesEnabled": {
        "extensibility": true,
        "optionalModuleNames": true
    },
    "moduleAliases": {
        "ts": {},
        "br": {}
    }
}
